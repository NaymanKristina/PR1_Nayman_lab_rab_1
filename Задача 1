# PR1_Nayman_lab_rab_1
<!DOCTYPE HTML> 
<html> 
 <head> 
   <title>лаба1</title> 
 </head> 
    <body> 

     <script> 
        var a; var b; var c;
        var a = prompt('Введите число а', ''); 
        var b = prompt('Введите число b', ''); 
        var c = prompt('Введите число c', ''); 
           if ( a >= 0)
             alert (a*a);
           else alert();
           if ( b >= 0)
             alert (b*b);
           else alert();
           if ( c >= 0)
             alert (c*c);
           else alert();
     </script> 
   </body> 
</html> 
